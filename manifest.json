{
	"manifest_version": 2,
	"name": "Master Web",
	"version": "0.0.1",

	"description": "Extension helps for learning how to work with different web sites",

	"icons": {
		"48": "icons/web-master.svg"
	},

	"permissions": ["webNavigation", "storage"],

	"browser_action": {
		"default_icon": {
			"48" : "icons/web-master.svg"
		},
		"default_title": "Master Web",
		"default_popup": "popup/popup.html"
	},

	"background": {
		"scripts": [ "background.js" ]
	}
}